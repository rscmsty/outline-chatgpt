{
  "manifest_version": 3,
  "name": "ChatGPT Outliner",
  "version": "0.1",
  "description": "Extracts the Q&A block from ChatGPT.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "host_permissions": [
    "https://*.chatgpt.com/*"
  ]
}